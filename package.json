{
  "name": "ts-react-boilerplate",
  "description": "A universal boilerplate for building web applications w/ TypeScript, React, Redux and more.",
  "main": "build/server.js",
  "scripts": {
    "precommit": "npm run lint",
    "prepush": "npm run build && npm test",
    "commitmsg": "validate-commit-msg",
    "clean": "rimraf build",
    "prebuild": "npm run clean -s",
    "build": "webpack --config config/webpack/index.js",
    "build:prod": "cross-env NODE_ENV=production npm run build",
    "postbuild": "webpack --config config/webpack/server.js",
    "prestart": "npm run build -s",
    "start": "node build/server.js",
    "start:prod": "cross-env NODE_ENV=production npm start",
    "test": "karma start config/test/karma.conf.js --single-run --no-auto-watch",
    "test:watch": "karma start config/test/karma.conf.js",
    "lint": "tslint -t stylish \"src/**/**.ts*\"",
    "lint:fix": "npm run lint -s -- --fix"
  },
  "author": "Crazy Factory <dev@crazy-factory.com>",
  "homepage": "https://github.com/crazyfactory/ts-react-boilerplate.git",
  "repository": {
    "type": "git",
    "url": "https://github.com/crazyfactory/ts-react-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/crazyfactory/ts-react-boilerplate/issues"
  },
  "keywords": [
    "react",
    "redux",
    "typescript",
    "webpack",
    "universal",
    "isomorphic",
    "starter",
    "kit",
    "boilerplate",
    "hot reload",
    "react-hot-loader",
    "enzyme",
    "mocha",
    "chai"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/base16": "^1.0.1",
    "@types/chai": "^3.4.34",
    "@types/enzyme": "^2.7.7",
    "@types/isomorphic-fetch": "0.0.33",
    "@types/mocha": "^2.2.38",
    "@types/node": "^7.0.0",
    "@types/react": "^15.0.18",
    "@types/react-addons-test-utils": "^0.14.16",
    "@types/react-dom": "^0.14.20",
    "@types/react-helmet": "^5.0.0",
    "@types/react-redux": "^4.4.38",
    "@types/react-router": "^3.0.3",
    "@types/react-router-redux": "^4.0.38",
    "@types/redux": "^3.6.31",
